{
  "component": {
    "toggle": {
      "dimension": {
        "track": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.config.track.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.config.track.height}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.md} + {component.toggle.config.track.step}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.toggle.dimension.handle.width.sm} * 2 + {component.toggle.config.handle.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.handle.width.md} * 2 + {component.toggle.config.handle.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.handle.width.lg} * 2 + {component.toggle.config.handle.inset} * 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.toggle.dimension.track.radius.md} - {component.toggle.config.track.radius.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.config.track.radius.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.radius.md} + {component.toggle.config.track.radius.step}",
              "type": "dimension"
            }
          }
        },
        "handle": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.track.height.sm} - {component.toggle.config.handle.inset} * 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.height.md} - {component.toggle.config.handle.inset} * 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.height.lg} - {component.toggle.config.handle.inset} * 2",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "({component.toggle.dimension.track.height.sm} - {component.toggle.config.handle.inset} * 2) * {component.toggle.config.handle.ratio}",
              "type": "dimension"
            },
            "md": {
              "value": "({component.toggle.dimension.track.height.md} - {component.toggle.config.handle.inset} * 2) * {component.toggle.config.handle.ratio}",
              "type": "dimension"
            },
            "lg": {
              "value": "({component.toggle.dimension.track.height.lg} - {component.toggle.config.handle.inset} * 2) * {component.toggle.config.handle.ratio}",
              "type": "dimension"
            },
            "pressed": {
              "sm": {
                "value": "{component.toggle.dimension.handle.width.sm} + {component.toggle.dimension.handle.width.sm} * {component.toggle.config.handle.pressed}",
                "type": "dimension"
              },
              "md": {
                "value": "{component.toggle.dimension.handle.width.md} + {component.toggle.dimension.handle.width.md} * {component.toggle.config.handle.pressed}",
                "type": "dimension"
              },
              "lg": {
                "value": "{component.toggle.dimension.handle.width.lg} + {component.toggle.dimension.handle.width.lg} * {component.toggle.config.handle.pressed}",
                "type": "dimension"
              }
            }
          },
          "offset": {
            "sm": {
              "value": "{component.toggle.dimension.handle.width.sm} / 2",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.handle.width.md} / 2",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.handle.width.lg} / 2",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.toggle.dimension.track.radius.sm} - {component.toggle.config.handle.inset}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.track.radius.md} - {component.toggle.config.handle.inset}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.track.radius.lg} - {component.toggle.config.handle.inset}",
              "type": "dimension"
            }
          }
        },
        "icon": {
          "height": {
            "sm": {
              "value": "{component.toggle.dimension.handle.height.sm} * 0.5",
              "type": "dimension"
            },
            "md": {
              "value": "{component.toggle.dimension.handle.height.md} * 0.5",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.toggle.dimension.handle.height.lg} * 0.5",
              "type": "dimension"
            }
          },
          "padding": {
            "sm": {
              "value": "({component.toggle.dimension.handle.width.sm} - {component.toggle.dimension.icon.height.sm}) / 2 + 0.0625rem",
              "type": "dimension"
            },
            "md": {
              "value": "({component.toggle.dimension.handle.width.md} - {component.toggle.dimension.icon.height.md}) / 2 + 0.125rem",
              "type": "dimension"
            },
            "lg": {
              "value": "({component.toggle.dimension.handle.width.lg} - {component.toggle.dimension.icon.height.lg}) / 2 + 0.0625rem",
              "type": "dimension"
            }
          }
        }
      },
      "color": {
        "track": {
          "unchecked": {
            "bg": {
              "default": {
                "value": "{app.color.component.unchecked.bg.translucent}",
                "type": "color"
              },
              "active": {
                "value": "{app.color.component.unchecked.bg.translucent}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "alpha",
                      "value": "{config.color.neutral.alpha.unchecked.bg.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              },
              "disabled": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          },
          "checked": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.bg.solid}",
                "type": "color"
              },
              "active": {
                "value": "{app.color.component.primary.bg.solid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "darken",
                      "value": "{config.color.primary.darken.active}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.onSolid}",
                "type": "color"
              }
            }
          }
        },
        "handle": {
          "bg": {
            "default": {
              "value": "{app.color.component.unchecked.fg.onSolid}",
              "type": "color"
            },
            "disabled": {
              "value": "{app.color.component.unchecked.fg.onSolid}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "alpha",
                    "value": "{config.color.neutral.alpha.bg.active}",
                    "space": "hsl"
                  }
                }
              }
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.unchecked.fg.solid}",
              "type": "color"
            }
          }
        }
      },
      "boxShadow": {
        "default": {
          "value": "{app.boxShadow.control.default}",
          "type": "boxShadow"
        },
        "pressed": {
          "value": [
            {
              "x": "0",
              "y": "0",
              "blur": "0.0625rem",
              "spread": "0",
              "color": "#0000004d",
              "type": "dropShadow"
            }
          ],
          "type": "boxShadow"
        }
      },
      "fontSize": {
        "icon": {
          "sm": {
            "value": "{component.toggle.dimension.icon.height.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{component.toggle.dimension.icon.height.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{component.toggle.dimension.icon.height.lg}",
            "type": "fontSizes"
          }
        }
      },
      "config": {
        "track": {
          "height": {
            "value": "{app.dimension.control.height.md} / 2",
            "type": "other",
            "description": "-0.5 for material design style"
          },
          "step": {
            "value": "0.5rem",
            "type": "other"
          },
          "radius": {
            "default": {
              "value": "0.5rem",
              "type": "other",
              "description": "for 100% roundness lower last value to 0"
            },
            "step": {
              "value": "0.125rem",
              "type": "other"
            }
          }
        },
        "handle": {
          "inset": {
            "value": "0.125rem",
            "type": "other"
          },
          "ratio": {
            "value": "1.5",
            "type": "other",
            "description": "ratio defines width → 1 = square / 1.5 = rectangle"
          },
          "pressed": {
            "value": "0.125",
            "type": "other"
          }
        }
      },
      "animate": {
        "handle": {
          "state": {
            "value": "0.5s ease-in-out",
            "type": "other"
          },
          "transform": {
            "value": "1s ease",
            "type": "other"
          }
        },
        "track": {
          "state": {
            "value": "0.5s ease",
            "type": "other"
          }
        }
      }
    }
  }
}