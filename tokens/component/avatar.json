{
  "component": {
    "avatar": {
      "typography": {
        "text": {
          "value": "{app.typography.control.text}",
          "type": "typography"
        }
      },
      "color": {
        "bg": {
          "default": {
            "value": "{app.color.component.secondary.bg.solid}",
            "type": "color"
          }
        },
        "fg": {
          "default": {
            "value": "{app.color.component.secondary.fg.onSolid}",
            "type": "color"
          }
        },
        "dimmer": {
          "default": {
            "value": "{app.color.surface.fg.default}",
            "type": "color",
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.1",
                  "space": "hsl"
                }
              }
            }
          }
        }
      },
      "dimension": {
        "height": {
          "sm": {
            "value": "{app.dimension.control.height.sm}",
            "type": "dimension"
          },
          "md": {
            "value": "{app.dimension.control.height.md}",
            "type": "dimension"
          },
          "lg": {
            "value": "{app.dimension.control.height.lg}",
            "type": "dimension"
          }
        },
        "radius": {
          "sm": {
            "value": "{app.dimension.control.radius.sm}",
            "type": "dimension"
          },
          "md": {
            "value": "{app.dimension.control.radius.md}",
            "type": "dimension"
          },
          "lg": {
            "value": "{app.dimension.control.radius.lg}",
            "type": "dimension"
          }
        },
        "border": {
          "sm": {
            "value": "{app.dimension.control.border.sm}",
            "type": "dimension"
          },
          "md": {
            "value": "{app.dimension.control.border.md}",
            "type": "dimension"
          },
          "lg": {
            "value": "{app.dimension.control.border.lg}",
            "type": "dimension"
          }
        }
      },
      "fontSize": {
        "text": {
          "sm": {
            "default": {
              "value": "{app.fontSize.control.text.sm}",
              "type": "fontSizes"
            },
            "inline": {
              "value": "{app.fontSize.control.text.sm}",
              "type": "fontSizes"
            }
          },
          "xs": {
            "default": {
              "value": "{app.fontSize.control.text.xs}",
              "type": "fontSizes"
            },
            "inline": {
              "value": "{app.fontSize.control.text.xs}",
              "type": "fontSizes"
            }
          },
          "md": {
            "default": {
              "value": "{app.fontSize.control.text.md}",
              "type": "fontSizes"
            },
            "inline": {
              "value": "{app.fontSize.control.text.md}",
              "type": "fontSizes"
            }
          },
          "lg": {
            "default": {
              "value": "{app.fontSize.control.text.lg}",
              "type": "fontSizes"
            },
            "inline": {
              "value": "{app.fontSize.control.text.lg}",
              "type": "fontSizes"
            }
          },
          "xl": {
            "default": {
              "value": "{app.fontSize.control.text.lg}",
              "type": "fontSizes"
            },
            "inline": {
              "value": "{app.fontSize.control.text.lg}",
              "type": "fontSizes"
            }
          }
        }
      },
      "letterSpacing": {
        "default": {
          "value": "{app.letterSpacing.control.text}",
          "type": "letterSpacing"
        }
      },
      "textCase": {
        "default": {
          "value": "{app.textCase.control}",
          "type": "textCase"
        }
      }
    }
  }
}