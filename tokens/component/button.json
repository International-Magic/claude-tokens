{
  "button": {
    "typography": {
      "text": {
        "sm": {
          "value": "{app.typography.control.text.sm}",
          "type": "typography"
        },
        "md": {
          "value": "{app.typography.control.text.md}",
          "type": "typography"
        },
        "lg": {
          "value": "{app.typography.control.text.lg}",
          "type": "typography"
        }
      },
      "symbol": {
        "sm": {
          "value": "{app.typography.control.symbol.sm}",
          "type": "typography"
        },
        "md": {
          "value": "{app.typography.control.symbol.md}",
          "type": "typography"
        },
        "lg": {
          "value": "{app.typography.control.symbol.lg}",
          "type": "typography"
        }
      }
    },
    "color": {
      "primary": {
        "solid": {
          "bg": {
            "default": {
              "value": "{app.color.component.primary.bg.solid.default}",
              "type": "color"
            },
            "hover": {
              "value": "{app.color.component.primary.bg.solid.hover}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.bg.solid.active}",
              "type": "color"
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.primary.fg.onSolid.default}",
              "type": "color"
            }
          }
        },
        "translucent": {
          "bg": {
            "default": {
              "value": "{app.color.component.primary.bg.translucent.default}",
              "type": "color"
            },
            "hover": {
              "value": "{app.color.component.primary.bg.translucent.hover}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.bg.translucent.active}",
              "type": "color"
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.primary.fg.onTranslucent.default}",
              "type": "color"
            }
          }
        },
        "ghost": {
          "bg": {
            "hover": {
              "value": "{app.color.component.primary.bg.translucent.hover}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.bg.translucent.active}",
              "type": "color"
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.primary.fg.solid.default}",
              "type": "color"
            }
          }
        },
        "outline": {
          "border": {
            "default": {
              "value": "{app.color.component.primary.border.solid.default}",
              "type": "color"
            },
            "hover": {
              "value": "{app.color.component.primary.border.solid.hover}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.border.solid.active}",
              "type": "color"
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.primary.fg.solid.default}",
              "type": "color"
            },
            "hover": {
              "value": "{app.color.component.primary.fg.solid.hover}",
              "type": "color"
            },
            "hover-copy": {
              "value": "{app.color.component.primary.fg.solid.active}",
              "type": "color"
            }
          }
        },
        "text": {
          "fg": {
            "default": {
              "value": "{app.color.component.primary.fg.solid.default}",
              "type": "color"
            },
            "hover": {
              "value": "{app.color.component.primary.fg.solid.default}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "darken",
                    "value": "{app.modifier.color.secondary.solid.hover}",
                    "space": "hsl"
                  }
                }
              }
            },
            "hover-copy": {
              "value": "{app.color.component.primary.fg.solid.default}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "darken",
                    "value": "{app.modifier.color.secondary.solid.active}",
                    "space": "hsl"
                  }
                }
              }
            }
          }
        }
      }
    },
    "dimension": {
      "height": {
        "sm": {
          "value": "{app.dimension.control.height.sm}",
          "type": "dimension"
        },
        "md": {
          "value": "{app.dimension.control.height.md}",
          "type": "dimension"
        },
        "lg": {
          "value": "{app.dimension.control.height.lg}",
          "type": "dimension"
        }
      },
      "padding": {
        "sm": {
          "value": "{app.dimension.control.padding.sm}",
          "type": "dimension"
        },
        "md": {
          "value": "{app.dimension.control.padding.md}",
          "type": "dimension"
        },
        "lg": {
          "value": "{app.dimension.control.padding.lg}",
          "type": "dimension"
        }
      },
      "gap": {
        "md": {
          "value": "{app.dimension.control.gap.md}",
          "type": "dimension"
        }
      },
      "radius": {
        "sm": {
          "value": "{app.dimension.control.radius.sm}",
          "type": "dimension"
        },
        "md": {
          "value": "{app.dimension.control.radius.md}",
          "type": "dimension"
        },
        "lg": {
          "value": "{app.dimension.control.radius.lg}",
          "type": "dimension"
        }
      }
    }
  }
}